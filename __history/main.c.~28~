#include <main.h>

#use rs232(baud=9600,Parity=N,xmit=PIN_C6,rcv=PIN_C7)

#INT_RDA
void ngatDongCo()
{
   char a;
   a = getc();
   switch(a)
   {
      case '1':
      {
         //printf("9");
         OUTPUT_BIT(PIN_B0,1);
         break;
      }
      case '0':
      {
         OUTPUT_BIT(PIN_B0,0);
         printf("8");
         break;
      }
   }
}



void main()
{
   enable_interrupts(int_rda);
   enable_interrupts(GLOBAL);
   while(TRUE)
   {
    
   }
}

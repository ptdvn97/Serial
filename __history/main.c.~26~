#include <main.h>
#define signal input(PIN_B1)
#use rs232(baud=9600,Parity=N,xmit=PIN_C6,rcv=PIN_C7)

#INT_RDA
void ngatDongCo()
{
   
   switch(signal)
   {
      case 1:
      {
         printf("hello");
         break;
      }
      case 0:
      {
         printf("Hell");
         break;
      }
   }
}



void main()
{
   enable_interrupts(int_rda);
   enable_interrupts(GLOBAL);
   while(TRUE)
   {
    
   }
}
